view, text, textarea, input {
  box-sizing: border-box;
}
image {
  display: block;
  height: auto;
  background-size: 100% 100%;
}
input {
  background: none;
}
page {
  background: #F7F7F7;
}
.null {
  height: 110rpx;
}
.footer_icon {
  width: 100%;
  height: 100rpx;
  background: #fff;
  position: fixed;
  left: 0;
  bottom: 0;
  display: flex;
  box-shadow: 0 0 20rpx rgba(0, 0, 0, 0.1);
  align-items: center;
  justify-content: space-between;
}
.footer_icon .mid {
  width: 98rpx;
  height: 98rpx;
  position: absolute;
  left: 50%;
  border-radius: 50%;
  background: #fff;
  -webkit-transform: translate(-50%, -50%);
          transform: translate(-50%, -50%);
  top: 40%;
  padding: 8rpx;
}
.footer_icon .mid image {
  width: 100%;
}
.footer_icon .v {
  display: flex;
  align-items: center;
  height: 100%;
}
.footer_icon .v view {
  width: 160rpx;
  height: 100%;
}
.footer_icon .v view.on text {
  color: #2765F2;
}
.footer_icon .v view image {
  width: 60rpx;
  display: block;
  margin: 0 auto;
}
.footer_icon .v view text {
  display: block;
  text-align: center;
  font-size: 24rpx;
  color: #999999;
}

