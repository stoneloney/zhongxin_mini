view, text, textarea, input {
  box-sizing: border-box;
}
image {
  display: block;
  height: auto;
  background-size: 100% 100%;
}
input {
  background: none;
}
page {
  background: #F7F7F7;
}
.content {
  width: 100%;
  min-height: 100vh;
  padding: 30rpx 24rpx;
  position: relative;
}
.content .search {
  width: 702rpx;
  position: relative;
  z-index: 99;
  margin: 0 auto;
  border: 1px solid #FFFFFF;
  height: 72rpx;
  border-radius: 20rpx;
  display: flex;
  align-items: center;
  padding-left: 13rpx;
}
.content .search image {
  width: 30rpx;
}
.content .search input {
  flex: 1;
  height: 100%;
  font-size: 28rpx;
  color: #fff;
  padding-left: 9rpx;
}
.content .search .s {
  color: #fff;
}
.content .search view {
  width: 108rpx;
  height: 58rpx;
  background: #FFFFFF;
  border: 1px solid #FFFFFF;
  box-shadow: 0rpx 0rpx 29rpx 0rpx rgba(19, 108, 242, 0.35);
  border-radius: 20rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  position: absolute;
  right: 5rpx;
  top: 50%;
  -webkit-transform: translateY(-50%);
          transform: translateY(-50%);
  font-size: 28rpx;
  z-index: 99;
  color: #2764F2;
}
.content .list {
  width: 702rpx;
  position: relative;
  z-index: 99;
  margin: 0 auto;
  margin-top: 27rpx;
}
.content .list .label {
  width: 100%;
  height: 185rpx;
  background: #FFFFFF;
  box-shadow: 0rpx 0rpx 29rpx 0rpx rgba(19, 108, 242, 0.35);
  border-radius: 20rpx;
  margin-bottom: 26rpx;
}
.content .list .label .bot {
  width: 100%;
  height: 61rpx;
  border-top: 1px solid #EEEEEE;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 0 36rpx 0 39rpx;
}
.content .list .label .bot .icon {
  display: flex;
  align-items: center;
}
.content .list .label .bot .icon image {
  width: 27rpx;
  margin-right: 13rpx;
}
.content .list .label .bot .icon text {
  font-size: 24rpx;
  color: #444A58;
}
.content .list .label .top {
  overflow: hidden;
  min-height: 123rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.content .list .label .top .mid {
  overflow: hidden;
}
.content .list .label .top .mid .f {
  display: block;
  margin: 0 auto;
  width: 34rpx;
  margin-bottom: 4rpx;
}
.content .list .label .top .mid .l {
  display: block;
  margin: 0 auto;
  width: 100rpx;
  margin-bottom: 6rpx;
}
.content .list .label .top .mid view {
  display: flex;
  align-items: center;
  justify-content: center;
}
.content .list .label .top .mid view image {
  width: 18rpx;
  margin-right: 6rpx;
}
.content .list .label .top .mid view text {
  font-size: 20rpx;
  color: #444A58;
}
.content .list .label .top .vi {
  overflow: hidden;
  width: 40%;
  padding-bottom: 5rpx;
}
.content .list .label .top .vi .time {
  display: block;
  text-align: center;
  font-size: 36rpx;
  font-weight: bold;
  margin-bottom: 10rpx;
  color: #222222;
}
.content .list .label .top .vi .name {
  display: block;
  text-align: center;
  font-size: 24rpx;
  color: #444A58;
  padding: 0 20rpx;
}
.content .date {
  width: 702rpx;
  position: relative;
  z-index: 99;
  background: #fff;
  margin: 0 auto;
  margin-top: 22rpx;
  overflow: hidden;
  border-radius: 20rpx;
  box-shadow: 0rpx 0rpx 29rpx 0rpx rgba(19, 108, 242, 0.35);
  height: 126rpx;
}
.content .date .icon {
  position: absolute;
  right: 0;
  top: 0;
  width: 106rpx;
  z-index: 999;
  padding-right: 33rpx;
  display: flex;
  align-items: center;
  justify-content: flex-end;
  height: 100%;
  background: linear-gradient(to right, white, white);
}
.content .date .icon image {
  width: 44rpx;
}
.content .date scroll-view {
  width: 100%;
  height: 100%;
  white-space: nowrap;
}
.content .date scroll-view view {
  display: inline-block;
  height: 100%;
  width: 121rpx;
  position: relative;
  padding: 24rpx 0 0 0;
}
.content .date scroll-view view::after {
  content: '';
  width: 1px;
  height: 102rpx;
  background: #EEEEEE;
  position: absolute;
  right: 0;
  top: 50%;
  -webkit-transform: translateY(-50%);
          transform: translateY(-50%);
}
.content .date scroll-view view .day {
  display: block;
  text-align: center;
  font-size: 28rpx;
  color: #000000;
  line-height: 1;
}
.content .date scroll-view view .des {
  display: block;
  text-align: center;
  font-size: 30rpx;
  color: #000000;
  line-height: 39rpx;
  opacity: 0.5;
}
.content .date scroll-view view .price {
  display: block;
  text-align: center;
  font-size: 22rpx;
  color: #000000;
  line-height: 1;
}
.content::after {
  content: '';
  width: 100%;
  height: 564rpx;
  position: absolute;
  left: 0;
  top: 0;
  background: linear-gradient(#2765F2, #F7F7F7);
}
.content .active {
  background: #60b1f1;
}
.content .navbar {
  overflow: hidden;
  width: 702rpx;
  margin: 0 auto;
  background: #fff;
  position: absolute;
  margin-top: 70rpx;
  box-shadow: 0rpx 0rpx 29rpx 0rpx rgba(19, 108, 242, 0.35);
  padding: 54rpx 0 9rpx 0;
  display: flex;
  margin-bottom: 43rpx;
  flex-wrap: wrap;
  z-index: 9999;
}
.content .navbar view {
  width: 33.33%;
  overflow: hidden;
  margin-bottom: 48rpx;
}
.content .navbar view image {
  display: block;
  margin: 0 auto;
  width: 98rpx;
  margin-bottom: 5rpx;
  height: 40rpx;
  width: 40rpx;
}
.content .navbar view text {
  display: block;
  font-size: 28rpx;
  color: #6C768D;
  text-align: center;
}
.fade_aircraft {
  overflow: hidden;
  width: 100%;
  border-radius: 20rpx 20rpx 0 0;
  background: #fff;
  position: relative;
  padding: 80rpx 35rpx;
}
.fade_aircraft .buttons {
  width: 100%;
  height: 97rpx;
  background: #2765F2;
  border-radius: 20rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 28rpx;
  margin-top: 35rpx;
  color: #FFFFFF;
}
.fade_aircraft .label {
  width: 100%;
}
.fade_aircraft .label .slide {
  overflow: hidden;
  width: 100%;
  background: #EEEEEE;
  border-radius: 10rpx;
  padding: 28rpx;
}
.fade_aircraft .label .slide .pic {
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
}
.fade_aircraft .label .slide .pic image {
  width: 306rpx;
  margin-top: 40rpx;
}
.fade_aircraft .label .slide .titles {
  display: block;
  font-size: 30rpx;
  font-weight: bold;
  letter-spacing: 1rpx;
  color: #444A58;
  margin-bottom: 30rpx;
}
.fade_aircraft .label .slide .des {
  display: block;
  font-size: 30rpx;
  color: #444A58;
  line-height: 45rpx;
}
.fade_aircraft .label .line {
  width: 100%;
  height: 91rpx;
  border-bottom: 1px solid #EEEEEE;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.fade_aircraft .label .line.on .icon view {
  background-image: url(data:image/png;base64,);
}
.fade_aircraft .label .line.on .down image {
  -webkit-transform: rotate(180deg);
          transform: rotate(180deg);
}
.fade_aircraft .label .line.on1 .down image {
  -webkit-transform: rotate(180deg);
          transform: rotate(180deg);
}
.fade_aircraft .label .line .down {
  display: flex;
  align-items: center;
}
.fade_aircraft .label .line .down text {
  font-size: 26rpx;
  opacity: 0.5;
  margin-right: 22rpx;
  color: #444A58;
}
.fade_aircraft .label .line .down image {
  width: 23rpx;
  transition: all .2s;
}
.fade_aircraft .label .line .icon {
  height: 100%;
  display: flex;
  align-items: center;
}
.fade_aircraft .label .line .icon view {
  width: 29rpx;
  height: 29rpx;
  background-image: url(data:image/png;base64,);
  background-size: 100% 100%;
  margin-right: 26rpx;
}
.fade_aircraft .label .line .icon text {
  font-size: 26rpx;
  color: #444A58;
}
.close {
  width: 36rpx;
  position: absolute;
  right: 45rpx;
  top: 27rpx;
  z-index: 999;
}
.title {
  display: block;
  font-size: 35rpx;
  font-weight: bold;
  letter-spacing: 1rpx;
  color: #444A58;
  text-align: center;
}

