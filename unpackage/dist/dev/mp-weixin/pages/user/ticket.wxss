view, text, textarea, input {
  box-sizing: border-box;
}
image {
  display: block;
  height: auto;
  background-size: 100% 100%;
}
input {
  background: none;
}
.ma_ticket {
  font-family: PingFang SC;
  color: #00153e;
}
.swiper-tab {
  background: #fff;
  z-index: 9;
  position: fixed;
  width: 100%;
  border-bottom: 1rpx solid #dee3f0;
  text-align: center;
  line-height: 80rpx;
}
.swiper-tab-list {
  display: inline-block;
  width: 50%;
  color: #00153e;
  font-size: 32rpx;
  font-weight: 500;
}
.on {
  color: #0D5EFF;
  border-bottom: 4rpx solid #0d5eff;
}
.address_a {
  width: 650rpx;
  margin: auto;
}
.address {
  width: 100%;
  font-family: PingFang SC;
  color: #00153e;
}
.img {
  background-repeat: no-repeat;
  background-size: cover;
  margin: auto;
  margin-top: 150rpx;
}
.img2 {
  font-size: 38rpx;
  font-weight: 400;
  margin-top: 65rpx;
  text-align: center;
}
.ticket-list {
  position: relative;
  top: 80rpx;
  display: block;
  width: 100%;
  overflow: hidden;
}
.coupon-item {
  background: #fff;
  padding: 20rpx;
  border-radius: 20rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-top: 20rpx;
}
.coupon-item.h {
  -webkit-filter: grayscale(100%);
          filter: grayscale(100%);
}
.coupon-item .coupon-item-left {
  display: flex;
  align-items: center;
}
.coupon-item .coupon-item-left .coupon-item-info {
  width: 170rpx;
  height: 170rpx;
  background-image: url("data:image/png;base64,");
  background-repeat: no-repeat;
  background-size: 100% 100%;
  color: #fff;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  flex-shrink: 0;
}
.coupon-item .coupon-item-left .coupon-item-info .coupon-item-money {
  font-size: 50rpx;
  font-weight: bold;
}
.coupon-item .coupon-item-left .coupon-item-info .coupon-item-money::first-letter {
  font-size: 24rpx;
  font-weight: normal;
}
.coupon-item .coupon-item-left .coupon-item-info .coupon-item-ask {
  font-size: 26rpx;
}
.coupon-item .coupon-item-left .coupon-item-content {
  color: #333;
  font-size: 28rpx;
  height: 160rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  margin-left: 20rpx;
  flex-shrink: 0;
}
.coupon-item .coupon-item-left .coupon-item-content .coupon-left-top .coupon-item-name {
  font-size: 30rpx;
  font-weight: bold;
}
.coupon-item .coupon-item-left .coupon-item-content .coupon-left-top .coupon-item-tip {
  font-size: 24rpx;
  color: #888888;
}
.coupon-item .coupon-item-left .coupon-item-content .coupon-left-bottom .coupon-item-time {
  font-size: 24rpx;
  color: #888888;
}
.coupon-item .coupon-item-right .coupon-item-botton {
  background: #ff4131;
  color: #fff;
  border-radius: 50rpx;
  padding: 12rpx 24rpx;
  font-size: 24rpx;
  flex-shrink: 0;
}

