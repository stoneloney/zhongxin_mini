view, text, textarea, input {
  box-sizing: border-box;
}
image {
  display: block;
  height: auto;
  background-size: 100% 100%;
}
input {
  background: none;
}
item page {
  background: #F7F7F7;
}
.content {
  width: 100%;
  height: 100vh;
  position: relative;
}
.content .box {
  width: 100%;
  position: fixed;
  left: 0;
  bottom: 0;
  z-index: 9999;
  padding: 0 24rpx 10rpx 24rpx;
}
.content .box .button {
  width: 100%;
  height: 97rpx;
  background: #2765F2;
  border-radius: 20rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 28rpx;
  color: #fff;
}
.content .box .list {
  overflow: hidden;
  width: 100%;
  background: #fff;
  border-radius: 20rpx;
  box-shadow: 0rpx 0rpx 29rpx 0rpx rgba(19, 108, 242, 0.35);
  margin-bottom: 16rpx;
  padding: 23rpx 28rpx;
}
.content .box .list scroll-view {
  width: 100%;
  height: 500rpx;
}
.content .box .list .label {
  width: 100%;
  display: flex;
  align-items: center;
  padding: 23rpx 0;
}
.content .box .list .label.dis {
  opacity: 0.5;
}
.content .box .list .label.on view {
  background-image: url(data:image/png;base64,);
}
.content .box .list .label view {
  width: 29rpx;
  height: 29rpx;
  background-image: url(data:image/png;base64,);
  background-size: 100% 100%;
  margin-right: 25rpx;
}
.content .box .list .label text {
  font-size: 32rpx;
  color: #444A58;
}

